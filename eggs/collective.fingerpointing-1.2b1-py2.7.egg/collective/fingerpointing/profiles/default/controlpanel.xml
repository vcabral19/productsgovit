<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.fingerpointing"
    purge="False">
    <configlet title="Finger Pointing"
        action_id="fingerpointing"
        appId="collective.fingerpointing"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@fingerpointing-settings"
        icon_expr="string:${portal_url}/++resource++collective.fingerpointing/fingerpointing-icon.png"
        visible="True"
        i18n:attributes="title">
      <permission>collective.fingerpointing: Setup</permission>
    </configlet>
</object>
