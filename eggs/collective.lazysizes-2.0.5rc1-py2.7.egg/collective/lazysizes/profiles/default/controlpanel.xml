<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.lazysizes"
    purge="False">
  <configlet title="lazysizes"
      action_id="lazysizes"
      appId="collective.lazysizes"
      category="Products"
      condition_expr=""
