<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="sc.photogallery"
    purge="False">
  <configlet title="Photo Gallery"
      action_id="photogallery"
      appId="sc.photogallery"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@photogallery-settings"
      icon_expr="string:${portal_url}/++resource++sc.photogallery/photogallery_icon.png"
      visible="True"
      i18n:attributes="title">
    <permission>sc.photogallery: Setup</permission>
  </configlet>
</object>
